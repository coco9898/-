.apppic-wrap {
    position: relative;
    width: 100%;
    height: 78rpx;
    line-height: 80rpx;
    margin: 30rpx auto;
    padding-left: 62rpx;
    box-sizing: border-box;
    font-size: 34rpx;
    color: #222;
}

.apppic {
    position: absolute;
    left: 20rpx;
    top: 20rpx;
    width: 34rpx;
    height: 34rpx;
}

.goapp {
    position: absolute;
    right: 20rpx;
    top: 0;
    color: #bbb;
    font-size: 27rpx;
    height: 70rpx;
    background: rgba(0,0,0,0);
}