.list-share {
    position: fixed;
    left: 0;
    bottom: 0;
    width: 100%;
    padding: 28rpx 0;
    display: flex;
    text-align: center;
    z-index: 300;
}

.ls-text {
    width: 300rpx;
    height: 60rpx;
    line-height: 60rpx;
    font-size: 28rpx;
    color: #222222;
}

.ls-btn {
    width: 180rpx;
    height: 60rpx;
    background-color: #5FC942;
    border-radius: 200rpx;
    margin-right: 48rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    color: #fff;
    position: relative;
}

.ls-sharebtn {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    border-radius: 200rpx;
    opacity: 0;
}

.ls-btn-circle {
    background-color: #FF8D2D;
}

.lsb-content {
    width: 120rpx;
    height: 48rpx;
    line-height: 48rpx;
    text-indent: 35rpx;
    font-size: 24rpx;
}

.lsbc-friend {
    background: url('data:image/png;base64,') no-repeat;
    background-size: 48rpx 48rpx;
    background-position: 0 0;
}

.lsbc-circle {
    width: 136rpx;
    text-indent: 35rpx;
    background: url('data:image/png;base64,') no-repeat;
    background-size: 48rpx 48rpx;
    background-position: 0 0;
}

.list-space {
    width: 100%;
    height: 116rpx;
    background-color: rgba(0,0,0,0);
}

.list-mask {
    position: fixed;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    background: rgba(0,0,0,0.5);
    z-index: 700;
}

.lm-wrap {
    width: 540rpx;
    margin: 370rpx auto;
}

.lm-image-wrap {
    width: 100%;
    height: 544rpx;
    border-radius: 10rpx;
    background-color: #fff;
    position: relative;
}

.lm-image {
    width: 100%;
    height: 100%;
    border-radius: 10rpx;
}

.lm-btn-wrap {
    height: 130rpx;
    box-sizing: border-box;
    padding: 30rpx;
    display: flex;
    justify-content: space-between;
}

.lm-btn {
    width: 226rpx;
    height: 70rpx;
    line-height: 70rpx;
    text-align: center;
    font-size: 30rpx;
    color: #222222;
    border-radius: 10rpx;
    text-indent: 40rpx;
}

.lmb-cancel {
    background: url('data:image/png;base64,') #fff no-repeat;
    background-size: 40rpx 40rpx;
    background-position: 56rpx 14rpx;
}

.lmb-save {
    background: url('data:image/png;base64,') #FFDA44 no-repeat;
    background-size: 40rpx 40rpx;
    background-position: 56rpx 14rpx;
}

.loading {
    width: 160rpx;
    height: 160rpx;
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%,-50%);
}

.one-px-t:before {
    content: "";
    height: 1px;
    left: 0;
    top: 0;
    width: 100%;
    transform: scaleY(0.4);
    box-sizing: border-box;
    pointer-events: none;
    background: #eee;
    position: absolute;
    z-index: 1;
}

.bgc-opc {
    background-color: rgba(0,0,0,0);
}

.space {
    width: 100%;
    height: 116rpx;
    background-color: rgba(0,0,0,0);
}

.txt-tip {
    font-size: 30rpx;
    margin-top: 8rpx;
    text-align: center;
    margin-bottom: 10rpx;
    width: 100%;
    color: #fff;
    background-color: transparent;
}