.space {
    width: 100%;
    height: 116rpx;
    background-color: rgba(0,0,0,0);
}

.list-share {
    position: fixed;
    left: 0;
    bottom: 0;
    width: 100%;
    padding: 28rpx 104rpx;
    display: flex;
    text-align: center;
    z-index: 800;
    background: url('data:image/gif;base64,') #fff no-repeat;
    background-size: 72rpx 74rpx;
    background-position: 26rpx 20rpx;
}

.ls-text {
    width: 425rpx;
    height: 60rpx;
    line-height: 62rpx;
    font-size: 28rpx;
    color: #222222;
    text-align: left;
    overflow: hidden;
    white-space: nowrap;
    margin-right: 10rpx;
}

.ls-btn {
    width: 180rpx;
    height: 60rpx;
    background-color: #5FC942;
    border-radius: 200rpx;
    margin-right: 48rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    color: #fff;
    position: relative;
}

.ls-sharebtn {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    border-radius: 200rpx;
    opacity: 0;
}

.lsb-content {
    width: 120rpx;
    height: 48rpx;
    line-height: 48rpx;
    text-indent: 35rpx;
    font-size: 24rpx;
}

.lsbc-friend {
    background: url('data:image/png;base64,') no-repeat;
    background-size: 48rpx 48rpx;
    background-position: 0 0;
}

.border::before {
    content: "";
    height: 1px;
    left: 0;
    top: 0;
    width: 100%;
    transform: scaleY(0.4);
    box-sizing: border-box;
    pointer-events: none;
    background: #eee;
    position: absolute;
    z-index: 1;
}