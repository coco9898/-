.coll-img {
    width: 100%;
    height: 100%;
}

.gif-list-scroll {
    background-color: #fff;
}

.giflist {
    margin: 0rpx 20rpx 0;
    padding-top: 20rpx;
    padding-bottom: 20rpx;
}

.empty image {
    width: 288rpx;
    height: 266rpx;
    margin: 0 auto 30rpx;
    display: block;
    background-color: rgba(0,0,0,0);
}

.empty text {
    font-size: 36rpx;
    color: #7E7E7E;
    text-align: center;
    display: block;
    background-color: rgba(0,0,0,0);
}

.holder {
    height: 160rpx;
}

.pic {
    position: relative;
    overflow: visible;
}

.air {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    z-index: 100;
}

.air-cancel {
    width: 128rpx;
    height: 64rpx;
    margin: -36rpx auto;
    background: url('data:image/png;base64,') no-repeat;
    background-size: 100% 100%;
}

.air-cancel-ceiling {
    width: 128rpx;
    height: 64rpx;
    margin: 194rpx auto;
    background: url('data:image/png;base64,') no-repeat;
    background-size: 100% 100%;
}

.share {
    position: absolute;
    bottom: 10rpx;
    right: 10rpx;
    width: 46rpx;
    height: 46rpx;
    background: url('data:image/png;base64,') no-repeat;
    background-size: 100% 100%;
    z-index: 90;
}