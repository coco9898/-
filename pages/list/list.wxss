.result {
    display: flex;
    justify-content: center;
    height: 150rpx;
    align-items: center;
    color: #999;
    background-color: #f9f9f9;
}

.result.hasResult {
    height: 0;
}

.titlebar {
    display: flex;
    border: none;
    color: #222;
    font-size: 34rpx;
    text-indent: 0;
    padding: 20rpx 0;
    margin: 0 20rpx;
    position: relative;
}

.titlebar .title {
    flex: 1;
    padding-left: 46rpx;
    background: url('data:image/png;base64,') no-repeat 0 50% / 36rpx;
    background-size: 34rpx 34rpx;
}

.section-hot {
    position: relative;
}

.pic navigator {
    width: 223rpx;
    height: 223rpx;
}

.pic view {
    width: 100%;
    height: 100%;
}

.pic image {
    width: 100%;
    height: 100%;
    display: block;
}

.section-hot {
    padding-bottom: 24rpx;
}

.section-newest .title {
    background: url('data:image/png;base64,') no-repeat 0 50% / 36rpx;
    background-size: 32rpx 32rpx;
}

.section-related .title {
    background: url('data:image/png;base64,') no-repeat 0 50% / 36rpx;
    background-size: 32rpx 32rpx;
}

.noresult {
    height: 266rpx;
    width: 100%;
    box-sizing: border-box;
    display: flex;
    background-color: transparent;
}

.noresult image {
    width: 288rpx;
    height: 266rpx;
    margin: 0 52rpx 0 70rpx;
}

.noresult .text {
    padding-top: 80rpx;
    font-size: 28rpx;
    color: #7e7e7e;
}

.noresult view {
    background-color: transparent;
}

.gomagic {
    height: 286rpx;
    margin: 20rpx;
    padding: 30rpx;
    border-radius: 10rpx;
    box-sizing: border-box;
    display: flex;
}

.gomagic-text {
    margin-left: 34rpx;
}

.gomagic-text .tit {
    font-size: 32rpx;
    color: #1e1e1e;
    line-height: 2;
}

.gomagic-text .intro {
    font-size: 28rpx;
    color: #888;
    line-height: 1.5;
}

.gomagic-img {
    width: 226rpx;
    height: 226rpx;
    background: #D8D8D8;
}

.gomagic-btn {
    width: 228rpx;
    height: 68rpx;
    text-align: center;
    line-height: 68rpx;
    font-size: 28rpx;
    color: #222222;
    background: #FFDA44;
    border-radius: 200rpx;
    margin-top: 50rpx;
}

.blacklist {
    height: 476rpx;
    width: 100%;
    text-align: center;
    box-sizing: border-box;
    padding-top: 336rpx;
    background: url('data:image/png;base64,') no-repeat;
    background-size: 294rpx 258rpx;
    background-position: 50% 40rpx;
}

.blacklist>view {
    background-color: rgba(0,0,0,0);
    font-family: PingFangSC-Regular;
    font-size: 36rpx;
    color: #7E7E7E;
    letter-spacing: 0;
}

.section-seg {
    margin: 30rpx 30rpx;
    padding-top: 20rpx;
    border-radius: 10rpx;
}

.word-seg {
    padding: 20rpx 0;
    text-align: center;
}

.relatelist {
    position: relative;
    margin: 20rpx 20rpx -22rpx;
}

.relate-title {
    padding-top: 20rpx;
    font-family: PingFangSC-Regular;
    font-size: 32rpx;
    color: #222222;
    letter-spacing: -0.78rpx;
    margin: 0 20rpx;
    z-index: 300;
}

.mask {
    position: absolute;
    bottom: 20rpx;
    right: 0rpx;
    width: 223rpx;
    height: 223rpx;
    opacity: 0.5;
    background: #222222;
    border-radius: 0.05rem;
    z-index: 5;
}

.gomore {
    position: absolute;
    bottom: 21rpx;
    right: 0px;
    box-sizing: border-box;
    z-index: 200;
    width: 223rpx;
    height: 223rpx;
    background-color: rgba(0,0,0,0);
    text-align: center;
}

.more-num {
    font-family: PingFangSC-Semibold;
    font-size: 72rpx;
    color: #FFFFFF;
    letter-spacing: 0;
    height: 200rpx;
    line-height: 200rpx;
    padding-right: 55rpx;
    margin-bottom: -47rpx;
    text-align: start;
    background: url('data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAACQAAAAkCAYAAADhAJiYAAAAAXNSR0IArs4c6QAAAHBJREFUWAnt1tsKQBEURVHb//+zyztNEaWmR8upY7TUjnSwSlujz6Ot0f7KXl459PKMP0TaCilEApTbIYVIgHI79J1QzEYIusmt3A6RrELfCW0P4/2msxfqkE89OMl9ZaSnkEIkQLkdUogEKLdDJFQB2VMMQvS8wS0AAAAASUVORK5CYII=') no-repeat;
    background-size: 36rpx 36rpx;
    background-position: 92% 50%;
    display: inline-block;
}

.more-text {
    margin: 0 auto;
    font-family: PingFangSC-Semibold;
    font-size: 14px;
    color: #FFFFFF;
}

.placeholder {
    margin: 0 0 20rpx 0;
}

.pageword {
    width: 100%;
    padding: 30rpx 24rpx;
    display: flex;
    box-sizing: border-box;
    background: #fff;
}

.pageword .pageword-item {
    height: 100rpx;
}

.pageword image.pageword-item {
    width: 100rpx;
    margin-right: 24rpx;
}

.pageword .content {
    flex: 1;
}

.pageword-title {
    color: #333;
    font-size: 32rpx;
    margin-bottom: 12rpx;
}

.pageword-tip {
    color: #bbb;
    font-size: 28rpx;
    padding-right: 12rpx;
    max-width: 540rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    display: inline-block;
}

.icon-right {
    width: 16rpx;
    background: url('data:image/png;base64,') no-repeat;
    background-size: 16rpx 26rpx;
    background-position: 0 38rpx;
}