.list-emopkg {
    background: #fff;
}

.result {
    display: flex;
    justify-content: center;
    height: 150rpx;
    align-items: center;
    color: #999;
    background-color: #f9f9f9;
}

.result.hasResult {
    height: 0;
}

.pic {
    position: relative;
}

.pic image {
    width: 100%;
    height: 100%;
    display: block;
}

.giflist {
    padding-bottom: 20rpx;
}

.lock {
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    height: 222rpx;
    background: url('data:image/png;base64,') no-repeat;
    background-size: 100% 100%;
    background-position: 0 0;
}

.emoji-tip {
    border: 4rpx solid #FFDA44;
    border-radius: 10rpx;
    height: 88rpx;
    line-height: 88rpx;
    margin: 20rpx 20rpx 0;
    padding: 0 30rpx;
    box-sizing: border-box;
    font-size: 28rpx;
    display: flex;
    align-items: center;
}

.emoji-tip view {
    background: none;
}

.emoji-tip .icon {
    height: 40rpx;
    width: 40rpx;
    background-size: 100% 100%;
    background-image: url(data:image/png;base64,);
    margin-right: 16rpx;
}

.emoji-tip .tit {
    flex-grow: 1;
}

.emoji-tip .arrow {
    height: 28rpx;
    width: 16rpx;
    background-size: 100% 100%;
    background-image: url(data:image/png;base64,);
}

.music-emoji {
    background: rgba(0,0,0,0.03);
    border-radius: 5px;
    padding: 24rpx 0 24rpx 24rpx;
    box-sizing: border-box;
    display: flex;
    margin: 20rpx 20rpx 0;
    align-items: center;
}

.music-emoji view {
    background: none;
}

.music-emoji .img {
    background-size: cover;
    height: 144rpx;
    width: 144rpx;
    margin-right: 20rpx;
    border-radius: 4rpx;
}

.music-emoji .mask {
    height: 100%;
    width: 100%;
    background-image: url(data:image/png;base64,);
    background-size: 26rpx 42rpx;
    background-repeat: no-repeat;
    background-position: center;
    background-color: rgba(0,0,0,0.4);
    border-radius: 4rpx;
}

.music-emoji .mid {
    flex-grow: 1;
    height: 144rpx;
}

.music-emoji .mid .tit {
    font-size: 32rpx;
    color: #333;
}

.music-emoji .mid .dur {
    color: #999;
    font-size: 28rpx;
    padding-top: 10rpx;
}

.music-emoji .btn {
    width: 116rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    position: relative;
}

.music-emoji .btn button {
    height: 100%;
    width: 100%;
    position: absolute;
    background: transparent;
}

.music-emoji .btn button::after {
    border: none;
}

.music-emoji .btn .center {
}

.music-emoji image {
    height: 40rpx;
    width: 40rpx;
    display: block;
    margin: 0 auto;
}

.music-emoji text {
    color: #333;
    font-size: 24rpx;
}

.prize-form {
    width: 100rpx;
    height: 100rpx;
    position: fixed;
    right: 32rpx;
    bottom: 150rpx;
}

.prize-form-item {
    width: 100%;
    height: 100%;
    position: absolute;
    left: 0;
    top: 0;
    z-index: 1;
}

.prize-form button {
    z-index: 20;
    opacity: 0;
}