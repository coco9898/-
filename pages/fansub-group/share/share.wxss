view {
    background: transparent;
}

.main-img {
    height: 450rpx;
    padding: 20rpx;
    box-sizing: border-box;
}

.main-img image {
    height: 100%;
    width: 100%;
}

.fixed {
    height: 80rpx;
    line-height: 80rpx;
    display: flex;
    justify-content: center;
    color: #999;
    font-size: 28rpx;
}

.fixed>view {
    margin: 0 20rpx;
    display: flex;
    align-items: center;
}

.fixed>view .icon {
    height: 36rpx;
    width: 36rpx;
    background-size: 100% 100%;
    margin-right: 10rpx;
}

.fixed .report .icon {
    background-image: url(data:image/png;base64,);
}

.fixed .save .icon {
    background-image: url(data:image/png;base64,);
}

.fixed {
    position: fixed;
    bottom: 68rpx;
    left: 0;
    width: 100%;
}

.btns {
    display: flex;
    padding: 0 20rpx;
}

.tip {
    color: #888;
    text-align: center;
    margin-bottom: 34rpx;
    font-size: 30rpx;
    line-height: 90rpx;
    display: flex;
    align-items: center;
    justify-content: center;
}

.tip .arrow {
    height: 34rpx;
    width: 48rpx;
    background-size: 100% 100%;
    background-image: url(data:image/png;base64,);
    margin-left: 20rpx;
}

.btns .home {
    height: 88rpx;
    width: 88rpx;
    background: #fff;
    border-radius: 10rpx;
    margin-right: 20rpx;
    background-size: 40rpx 40rpx;
    background-position: center;
    background-repeat: no-repeat;
    background-image: url(data:image/png;base64,);
}

.btns .create {
    height: 88rpx;
    line-height: 88rpx;
    flex-grow: 1;
    font-size: 32rpx;
    color: #333333;
    text-align: center;
    border-radius: 8rpx;
    background: #FFDA44;
    margin: 0 auto;
    position: relative;
    align-items: center;
    justify-content: center;
    display: flex;
}

.btns .create .pen {
    margin-right: 20rpx;
    height: 40rpx;
    width: 40rpx;
    background-size: 100% 100%;
    background-image: url(data:image/png;base64,);
}