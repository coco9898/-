view {
    background: transparent;
}

.main-img {
    padding: 20rpx;
    box-sizing: border-box;
    font-size: 0;
}

.main-img image {
    width: 100%;
    height: 404rpx;
}

.btns {
    height: 80rpx;
    line-height: 80rpx;
    display: flex;
    justify-content: center;
    color: #999;
    font-size: 28rpx;
    padding-top: 250rpx;
    padding-bottom: 100rpx;
}

.btns>view {
    margin: 0 20rpx;
    display: flex;
    align-items: center;
}

.btns>view .icon {
    height: 36rpx;
    width: 36rpx;
    background-size: 100% 100%;
    margin-right: 10rpx;
}

.btns .report .icon {
    background-image: url(data:image/png;base64,);
}

.btns .save .icon {
    background-image: url(data:image/png;base64,);
}

.fixed .tip {
    color: #888;
    text-align: center;
    margin-bottom: 34rpx;
    font-size: 30rpx;
}

.create {
    padding: 20rpx;
    box-sizing: border-box;
    position: relative;
}

.btns-big {
    width: 100%;
    height: 88rpx;
    display: flex;
    justify-content: space-between;
}

.home {
    width: 88rpx;
    height: 100%;
    border-radius: 10rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    margin-right: 20rpx;
    background-color: #fff;
}

.home image {
    width: 40rpx;
    height: 40rpx;
}

.create-too {
    flex: 1;
    height: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
    background-color: #ffda44;
    border-radius: 10rpx;
}

.create-too image {
    width: 36rpx;
    height: 36rpx;
    margin-right: 22rpx;
}

.create-too text {
    font-size: 32rpx;
    color: #333;
}

.guide-wrap {
    width: 567rpx;
    position: absolute;
    left: 147rpx;
    top: 130rpx;
    display: flex;
    justify-content: center;
}

.guide {
    display: flex;
    justify-content: center;
}

.guide text {
    margin-right: 14rpx;
    color: #888;
    font-size: 30rpx;
    max-width: 526rpx;
}

.guide image {
    width: 40rpx;
    height: 35rpx;
}