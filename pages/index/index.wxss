.section {
    margin-bottom: 20rpx;
    position: relative;
}

.titlebar {
    display: flex;
    border: none;
    color: #222;
    text-indent: 0;
    padding: 20rpx 0;
    margin: 0 20rpx;
    position: relative;
    height: 44rpx;
}

.titlebar .title {
    flex: 1;
    padding-left: 46rpx;
    font-size: 34rpx;
    background-size: 34rpx 34rpx;
}

.banner {
    background-color: #ffda44;
    width: 100%;
    background-size: 100%;
    position: relative;
}

.banner image {
    width: 100%;
    height: 0;
    display: block;
}

.section-hot {
    box-sizing: border-box;
    position: relative;
    overflow: hidden;
    padding: 30rpx 20rpx;
}

.hot-titlebar {
    display: flex;
    color: #bbb;
    font-size: 32rpx;
}

.hot-titlebar .title {
    flex: 1;
}

.change-btn {
    text-align: end;
}

.change-btn image {
    width: 40rpx;
    height: 40rpx;
    vertical-align: middle;
}

.change-btn text {
    vertical-align: middle;
    padding-left: 4rpx;
    color: #bbb;
    font-size: 32rpx;
}

.word {
    display: inline-block;
    font-size: 0;
    width: 33%;
    color: #333;
    margin-top: 20rpx;
    position: relative;
    text-align: center;
}

.word button {
    position: absolute;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    opacity: 0;
    border-radius: 0;
    margin: 0;
    padding: 0;
}

.word-item {
    font-size: 32rpx;
    vertical-align: middle;
    display: inline-block;
}

.word-tag {
    display: inline-block;
    vertical-align: middle;
    font-size: 18rpx;
    color: #fff;
    padding: 2rpx 4rpx;
    margin-left: 6rpx;
}

.word-tag.new {
    background-color: #8DD049;
}

.word-tag.hot {
    background-color: #F34848;
}

.word-tag.selfmade {
    background-color: #FF7400;
}

.word-tag.reco {
    background-color: #2159ED;
}

.section-banners {
    padding: 30rpx 0;
}

.section-recommend .title {
    background: url('data:image/png;base64,') no-repeat 0 50% / 36rpx;
    background-size: 34rpx 34rpx;
}

.section-newest .title {
    background: url('data:image/png;base64,') no-repeat 0 50% / 36rpx;
    background-size: 34rpx 34rpx;
}

.pic navigator {
    width: 223rpx;
    height: 223rpx;
}

.pic image {
    width: 100%;
    height: 100%;
    display: block;
}

.pic.placeholder {
    margin-bottom: 0rpx;
}

.section-footer {
    width: 100%;
    height: 50px;
    line-height: 50px;
    background-color: #fff;
    padding-bottom: 120rpx;
}

.link {
    font-size: 10px;
    color: #ccc;
    text-align: center;
}

.link-hover {
    color: #999;
}

.wxchat {
    position: fixed;
    bottom: 30rpx;
    right: 30rpx;
    width: 290rpx;
    height: 362rpx;
    background: rgba(0,0,0,0);
}

.list-reco {
    text-align: left;
}

.list-reco .list-reco__item {
    margin: 0 5rpx 20rpx;
    position: relative;
}

.titlebar .hotsearch {
    padding-left: 0rpx;
}

.bottom-fixed {
    position: fixed;
    left: 30rpx;
    bottom: 28rpx;
    display: flex;
    background: rgba(0,0,0,0);
    transition: bottom .5s;
}

.bottom-fixed-show {
    bottom: 28rpx;
}

.bottom-fixed-hide {
    bottom: -88rpx;
}

.helper {
    position: relative;
    margin-right: 30rpx;
    width: 262rpx;
    height: 88rpx;
    line-height: 88rpx;
    transform: rotate(-360deg);
    background: #FFFFFF;
    box-shadow: 0 0 20rpx 0 rgba(0,0,0,0.10);
    border-radius: 10rpx;
    font-size: 28rpx;
    color: #333333;
    letter-spacing: 1.5rpx;
    text-indent: 85rpx;
    background: url('data:image/png;base64,') no-repeat #fff;
    background-size: 40rpx 40rpx;
    background-position: 28rpx 23rpx;
    z-index: 900;
}

.helper-btn {
    position: absolute;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    opacity: 0;
    z-index: 901;
}

.collector {
    width: 390rpx;
    height: 88rpx;
    line-height: 88rpx;
    transform: rotate(-360deg);
    background: #FFFFFF;
    box-shadow: 0 0 20rpx 0 rgba(0,0,0,0.10);
    border-radius: 10rpx;
    font-size: 28rpx;
    color: #333333;
    letter-spacing: 1.5rpx;
    text-indent: 140rpx;
    background: url('data:image/png;base64,') no-repeat #fff;
    background-size: 40rpx 40rpx;
    background-position: 56rpx 24rpx;
    z-index: 900;
}

.section-trick .title {
    background-size: 34rpx 34rpx;
}

.list-trick {
    margin: 0 20rpx;
    overflow: hidden;
    min-height: 200rpx;
}

.list-trick__item {
    display: flex;
    height: 162rpx;
    margin: 20rpx 0;
    line-height: 1;
}

.list-trick__item .image {
    width: 162rpx;
    height: 162rpx;
}

.list-trick__item .text {
    flex: 1;
    margin-left: 20rpx;
}

.list-trick__item .trick-name {
    font-size: 32rpx;
    color: #222;
}

.list-trick__item .trick-desc {
    font-size: 28rpx;
    color: #999;
    line-height: 1.5;
    margin-top: 13rpx;
}

.lock {
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    height: 162rpx;
    background: url('data:image/png;base64,') no-repeat;
    background-size: 100% 100%;
    background-position: 0 0;
}

.section-make {
}

.section-make .title {
    background: url('data:image/png;base64,') no-repeat 0 50% / 36rpx;
    background-size: 36rpx 36rpx;
}

.section-make .list-wrap {
    box-sizing: border-box;
    padding: 20rpx 20rpx 0;
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
}

.section-make .list-wrap .item {
    width: 344rpx;
    font-size: 0;
}

.section-make .list-wrap .item image {
    width: 100%;
    height: 230rpx;
    border-radius: 10rpx;
}

.section-make .list-wrap .item view {
    line-height: 80rpx;
    font-size: 28rpx;
    color: #222;
}