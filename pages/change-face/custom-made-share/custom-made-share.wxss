.cusmade-share {
    height: 100%;
    width: 100%;
    position: relative;
}

.userinfo {
    display: flex;
    padding: 296rpx 120rpx 16rpx;
    position: relative;
}

.userinfo image {
    width: 54rpx;
    height: 54rpx;
    border-radius: 216rpx;
    display: inline-block;
    margin-right: 10rpx;
}

.userinfo .nick {
    flex: 1;
    height: 60rpx;
    line-height: 68rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    font-size: 35rpx;
    color: #fff;
    letter-spacing: 2rpx;
}

.pic-wrap {
    position: relative;
    padding: 20rpx;
    box-sizing: border-box;
    width: 100%;
    height: 750rpx;
}

.pic {
    width: 100%;
    height: 710rpx;
    margin: 0;
}

.btn-fixed {
    position: fixed;
    bottom: 60rpx;
    left: 0;
    width: 100%;
    height: 88rpx;
}

.btn-wrap {
    display: flex;
    justify-content: space-between;
    width: 670rpx;
    height: 88rpx;
    margin: 0 auto;
}

.btn {
    width: 344rpx;
    height: 88rpx;
    font-size: 32rpx;
    color: #333;
    line-height: 94rpx;
    text-align: center;
    box-sizing: border-box;
    padding-left: 48rpx;
    position: relative;
    border-radius: 10rpx;
}

.share {
    position: relative;
}

.share image {
    position: absolute;
    left: 44rpx;
    top: 24rpx;
    width: 40rpx;
    height: 40rpx;
}

.save {
    background: url('data:image/png;base64,') no-repeat #fff;
    background-size: 40rpx 40rpx;
    background-position: 36rpx 24rpx;
}

.upload {
    background: url('data:image/png;base64,') no-repeat #fff;
    background-size: 40rpx 40rpx;
    background-position: 70rpx 24rpx;
}

.share button {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    border-radius: 286rpx;
    opacity: 0;
    background-color: rgba(0,0,255,0.5);
}

.space {
    width: 100%;
    height: 32rpx;
}

.loading {
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%,-50%);
    width: 140rpx;
    height: 140rpx;
}

.report-wrap {
    padding-top: 50rpx;
    width: 100%;
}

.report {
    font-size: 28rpx;
    color: #999;
    text-align: center;
    line-height: 41rpx;
    width: 150rpx;
    height: 39rpx;
    background: url('data:image/png;base64,') no-repeat;
    background-size: 36rpx 36rpx;
    background-position: 0 0;
    margin: 0 auto;
    padding-left: 28rpx;
}

.pre-mask {
    position: absolute;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    opacity: 0;
}

.mask {
    position: fixed;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    background: rgba(0,0,0,0.5);
    z-index: 700;
}

.mode-wrap {
    position: absolute;
    bottom: 0;
    left: 0;
    width: 100%;
    padding: 0 20rpx 20rpx;
    box-sizing: border-box;
}

.mode-content {
    border-radius: 18rpx;
    overflow: hidden;
}

.mode {
    height: 90rpx;
    font-size: 36rpx;
    line-height: 90rpx;
    text-align: center;
    color: #222;
    position: relative;
}

.one-px-t:before {
    background: #999;
}

.cancel {
    margin-top: 20rpx;
    border-radius: 18rpx;
}

.guide {
    font-size: 24rpx;
    line-height: 39rpx;
    padding: 14rpx;
    box-sizing: border-box;
    color: #777;
}

.change-wrap {
    width: 100%;
    z-index: 2;
}

.change-content {
    position: relative;
    margin: 0 auto;
    display: flex;
    width: 100%;
    justify-content: space-between;
    padding: 20rpx;
    box-sizing: border-box;
}

.change {
    height: 88rpx;
    line-height: 90rpx;
    text-align: center;
    font-size: 32rpx;
    color: #333;
    border-radius: 8rpx;
    background: url('data:image/png;base64,') no-repeat #ffda44;
    background-size: 40rpx 40rpx;
    background-position: 76rpx 24rpx;
    padding-left: 60rpx;
    box-sizing: border-box;
}

.save-wrap {
    position: absolute;
    width: 100%;
    text-align: center;
    bottom: 94rpx;
}

.save {
    background: url('data:image/png;base64,') no-repeat;
    background-size: 36rpx 36rpx;
    background-position: 0 0;
    margin-left: 30rpx;
    width: 250rpx;
}

.report {
    display: inline-block;
    font-size: 28rpx;
    color: #999;
    text-align: center;
}

.share-rep {
    position: relative;
}

.share-rep image {
    position: absolute;
    left: 44rpx;
    top: 24rpx;
    width: 40rpx;
    height: 40rpx;
}

.share-rep button {
    position: absolute;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    opacity: 0;
}

.guide-wrap {
    width: 567rpx;
    position: absolute;
    left: 93rpx;
    top: 130rpx;
    display: flex;
    justify-content: center;
}

.guide {
    display: flex;
    justify-content: center;
}

.guide text {
    margin-right: 14rpx;
    color: #888;
    font-size: 30rpx;
    max-width: 526rpx;
}

.guide image {
    width: 40rpx;
    height: 35rpx;
}