.custom-made {
    display: flex;
    flex-direction: column;
    height: 100%;
}

.section-source:last-child {
    margin-bottom: 0rpx;
}

.pkgbar {
    display: flex;
    border: none;
    color: #222;
    font-size: 28rpx;
    text-indent: 0;
    padding: 20rpx 0;
    margin: 0 20rpx;
    position: relative;
}

.pkgbar .title {
    flex: 1;
    background-size: 32rpx 32rpx;
    font-size: 34rpx;
}

.pic {
    position: relative;
    width: 344rpx;
    height: 344rpx;
}

.pic image {
    width: 100%;
    height: 100%;
    display: block;
    background-color: #ffffff;
}

.loading-pic {
    position: absolute;
    left: 0;
    top: 0;
}

.change-wrap {
    position: fixed;
    bottom: 70rpx;
    width: 100%;
    z-index: 2;
}

.change {
    width: 290rpx;
    height: 88rpx;
    line-height: 90rpx;
    text-align: center;
    font-size: 32rpx;
    color: #333;
    margin: 0 auto;
    box-shadow: 0 0 20rpx 0 rgba(0,0,0,0.10);
    border-radius: 8rpx;
    background: url('data:image/png;base64,') no-repeat #FFDA44;
    background-size: 40rpx 40rpx;
    background-position: 50rpx 24rpx;
    padding-left: 62rpx;
    box-sizing: border-box;
}

.holder {
    width: 100%;
    height: 60rpx;
    text-align: center;
    font-size: 30rpx;
    color: #bbb;
}

.mask {
    position: fixed;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    background: rgba(0,0,0,0.5);
    z-index: 700;
}

.mode-wrap {
    position: absolute;
    bottom: 0;
    left: 0;
    width: 100%;
    padding: 0 20rpx 20rpx;
    box-sizing: border-box;
}

.mode-content {
    border-radius: 18rpx;
    overflow: hidden;
}

.mode {
    height: 90rpx;
    font-size: 36rpx;
    line-height: 90rpx;
    text-align: center;
    color: #222;
    position: relative;
}

.one-px-t:before {
    background: #999;
}

.guide {
    font-size: 24rpx;
    line-height: 39rpx;
    padding: 14rpx;
    box-sizing: border-box;
    color: #777;
}

.guide text {
}

.cancel {
    margin-top: 20rpx;
    border-radius: 18rpx;
}

.formbutton {
    z-index: 2;
    opacity: 0;
    position: absolute;
    background-color: rgba(0,255,0,0.5);
    left: 231rpx;
    top: 0;
    height: 92rpx;
}